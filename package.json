{
  "name": "maai-cursor",
  "displayName": "Maai Cursor",
  "description": "Enhance vertical moving of cursor.",
  "version": "1.0.0",
  "repository": {
    "url": "https://github.com/elzup/vscode-maai-cursor.git",
    "type": "git"
  },
  "publisher": "anozon",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "maaiCursor.toggle",
        "title": "Maai Cursor: Toggle on/off"
      }
    ],
    "configuration": {
      "title": "Maai Cursor",
      "properties": {
        "maaiCursor.distance": {
          "type": "number",
          "description": "Number of lines of jump",
          "minimum": 1,
          "maximum": 100,
          "default": 5
        },
        "maaiCursor.mode": {
          "type": "string",
          "enum": [
            "point",
            "line",
            "para"
          ],
          "description": "Hilight mode",
          "default": "point"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "release": "./publish.sh"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "16.11.7",
    "@types/vscode": "^1.62.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.2.0",
    "eslint-config-nzap": "^5.0.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "license": "MIT"
}
